<h2><%= @model.title %>のテスト</h2>

<section>
  <% @model.methods.each do |method| %>
    <dl>
      <dt><%= method.description %></dt>
      <dd>
	<ul>
          <% method.contexts.each do |context| %>
            <li>
	      <%= context.description %>
	      
	    </li>
          <% end %>
	</ul>
      </dd>
    <dl>
  <% end %>
</section>

